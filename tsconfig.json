{
    "compilerOptions": {
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "strict": true,
        "paths": {
            "components/*": [
                "src/components/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ],
    "typeAcquisition": {
        "include": [
            "lodash",
            "vue",
        ]
    },
    "rules": {
        "no-console": false
    }
}
